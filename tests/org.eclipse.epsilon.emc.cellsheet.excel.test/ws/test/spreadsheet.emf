@namespace(
	uri="http://a.b.c/x/y/Z",
	prefix="p")
package top;

class Book {
	val Sheet[*]#book sheets;
}

class Sheet {
	ref Book#sheets book;
	val Row[*]#sheet rows;
	val Col[*]#sheet cols;
	val Cell[*]#sheet cells;
}

class Row {
	attr int index;
	ref Sheet#rows sheet;
	ref Cell[*]#row cells;
}

class Col {
	attr int index;
	ref Sheet#cols sheet;
	ref Cell[*]#col cells;
}

class Cell {
	ref Sheet#cells sheet;
	ref Row#cells row;
	ref Col#cells col;
}