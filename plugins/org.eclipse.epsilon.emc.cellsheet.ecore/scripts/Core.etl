import "Common.etl";

rule Book2Book
	transform s : Excel!Book
	to t : Emf!Book
	extends HasA1, HasId {

	t.name := s.name;
}

rule Sheet2Sheet
	transform s : Excel!Sheet
	to t : Emf!Sheet
	extends HasA1, HasBook {

	t.name := s.name;
	t.index := s.index;
}

rule Row2Row
	transform s : Excel!Row
	to t : Emf!Row
	extends HasA1, HasSheet {

	t.index := s.index;
}

rule Cell2Cell
	transform s : Excel!Cell
	to t : Emf!Cell 
	extends HasA1, HasRow {
	
	t.index := s.colIndex;
	t.a1Index := s.a1Col;
}